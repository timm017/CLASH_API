extends layout

block content
  ul 
    each d, i in memberData
      li
        a(href="./member?usertag=" + d.tag.replace('#','')) #{d.name}
        //- a(href="./member?usertag=" + HIII) TT
        if d.role == "admin"
          span(class='role')  (elder)
        else 
          span(class='role')  (#{d.role})
